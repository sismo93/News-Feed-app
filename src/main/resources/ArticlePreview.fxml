<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="770.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.be.ac.ulb.g05.Controller.ArticlePreviewController">
   <children>
      <HBox fx:id="articleTitleContainer" layoutX="14.0" layoutY="14.0" prefHeight="69.0" prefWidth="1255.0">
         <children>
            <Label fx:id="articleTitleArea" prefHeight="88.0" prefWidth="1255.0" wrapText="true">
               <font>
                  <Font name="System Bold" size="23.0" />
               </font>
            </Label>
         </children>
      </HBox>
      <HBox fx:id="articlePreviewContentContainer" layoutX="14.0" layoutY="113.0" prefHeight="430.0" prefWidth="1255.0">
         <children>
            <Label fx:id="articlePreviewContentArea" prefHeight="530.0" prefWidth="1265.0" wrapText="true"/>
         </children>
      </HBox>
      <HBox fx:id="buttonsContainer" layoutX="14.0" layoutY="556.0" prefHeight="46.0" prefWidth="1247.0">
         <children>
            <JFXButton fx:id="readArticle" style="-fx-background-color: #018786;" buttonType="RAISED" prefHeight="30.0" prefWidth="100.0" text="READ">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="openLink" style="-fx-background-color: #018786;" buttonType="RAISED" prefHeight="30.0" prefWidth="150.0" text="OPEN LINK">
               <HBox.margin>
                  <Insets right="10.0" />
               </HBox.margin>
            </JFXButton>
            <JFXButton fx:id="copyLink" style="-fx-background-color: #018786;" buttonType="RAISED" prefHeight="30.0" prefWidth="150.0" text="COPY LINK" />
            <JFXButton fx:id="deleteFromFeed" style="-fx-background-color: #018786;" buttonType="RAISED" prefHeight="31.0" prefWidth="168.0" text="DELETE FROM FEED">
               <HBox.margin>
                  <Insets left="650.0" />
               </HBox.margin>
            </JFXButton>
         </children>
      </HBox>
   </children>
</AnchorPane>
