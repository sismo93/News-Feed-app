<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>


<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.be.ac.ulb.g05.Controller.AddFromWebSiteController">
    <Label layoutX="485.0" layoutY="77.0" text="Add article to feed">
        <font>
            <Font name="Arial Bold" size="39.0"/>
        </font>
    </Label>
    <StackPane fx:id="stackPane" layoutX="446.0" layoutY="188.0" prefHeight="345.0" prefWidth="420.0">
        <Pane prefHeight="328.0" prefWidth="431.0">
            <Label layoutX="73.0" layoutY="42.0" prefHeight="21.0" prefWidth="53.0" text="Source:"/>
            <JFXComboBox fx:id="SourceArticleBox" focusColor="#2196f3" layoutX="126.0" layoutY="37.0"
                         prefHeight="31.0" prefWidth="192.0"/>
            <Label layoutX="62.0" layoutY="93.0" text="Category:"/>
            <JFXComboBox fx:id="CategoryBox" focusColor="#2196f3" layoutX="126.0" layoutY="88.0"
                         prefHeight="31.0" prefWidth="192.0"/>
            <JFXButton fx:id="AddButton" buttonType="RAISED" layoutX="73.0" layoutY="173.0"
                       onAction="#OnButtonPressed" prefHeight="31.0" prefWidth="275.0" ripplerFill="WHITE"
                       style="-fx-background-color: #2196f3; -fx-background-radius: 20;" text="ADD"
                       textFill="WHITE">
                <font>
                    <Font name="System Bold" size="15.0"/>
                </font>
            </JFXButton>
        </Pane>
    </StackPane>
</AnchorPane>
